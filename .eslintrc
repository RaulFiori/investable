{
  "extends": ["react-app", "prettier", "airbnb", "prettier/react"],
  "parser": "babel-eslint",
  "plugins": ["prettier", "react", "react-hooks"],
  "rules": {
    "react/jsx-props-no-spreading": "off",
    "import/prefer-default-export": "off",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "all",
        "arrowParens": "avoid",
        "printWidth": 80,
        "tabWidth": 2
      }
    ]
  }
}
